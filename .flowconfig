[ignore]
.*/node_modules/*
.*/.git/.*

[libs]
interfaces

[options]
module.name_mapper='.+\.s?css$' -> 'CSSModule'
module.name_mapper='.*\(.graphql\)' -> 'empty/object'
module.system=haste
unsafe.enable_getters_and_setters=true
module.name_mapper='^\$rocket\/framework$' -> '<PROJECT_ROOT>/src/providers/frameworkComponent.js'
module.name_mapper='^\$rocket\/style$' -> '<PROJECT_ROOT>/src/providers/frameworkStyle.js'
module.name_mapper='^\$rocket\/\(.*\)' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^\$images\/\(.*\)' -> '<PROJECT_ROOT>/src/img/\1'
module.name_mapper='^\$themesPath\/\(.*\)' -> '<PROJECT_ROOT>/themes/rocket/\1'